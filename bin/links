#!/bin/bash

loc_dist=binaries/dist
loc_rel=binaries/rel
loc_tiny=binaries/tiny
loc_dbg=binaries/dbg

for type in $(echo examples third_party tool build test)
do
 echo "| ${type} | dist | release | tiny | debug |"
 echo "| ------ | "
 for file in `find binaries/dist/${type}/ -name '*.com'`
 do
  echo -n "| ${file} "
  echo -n "| [ðŸ’¾](https://github.com/marcelmaatkamp/cosmopolitan/blob/main/binaries/dist/${file}) "
  echo -n "| [ðŸ’¾](https://github.com/marcelmaatkamp/cosmopolitan/blob/main/binaries/rel/${file}) "
  echo -n "| [ðŸ’¾](https://github.com/marcelmaatkamp/cosmopolitan/blob/main/binaries/tiny/${file}) "
  echo -n "| [ðŸ’¾](https://github.com/marcelmaatkamp/cosmopolitan/blob/main/binaries/dbg/${file}) "
  echo "|"
 done
 echo
done
