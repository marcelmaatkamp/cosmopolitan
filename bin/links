#!/bin/bash
for dir in $(echo examples third_party tool build test)
do
 echo "| ${dir} |"
 echo "| ------ |"
 for file in `find data/application/${dir} -name '*.com'`
 do
  echo "| [${file/data\/application\//}](https://raw.githubusercontent.com/marcelmaatkamp/cosmopolitan/raw/main/${file}) |"
 done
 echo
done
